{
  "name": "fleehy",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/frontend",
    "apps/backend"
  ],
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:frontend": "npm --prefix apps/frontend run dev",
    "dev:backend": "npm --prefix apps/backend run dev",
    "build": "npm-run-all -s build:*",
    "build:frontend": "npm --prefix apps/frontend run build",
    "build:backend": "npm --prefix apps/backend run build",
    "start:frontend": "npm --prefix apps/frontend start",
    "start:backend": "npm --prefix apps/backend start"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}